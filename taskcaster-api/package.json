{
    "name": "taskcaster-api",
    "version": "1.0.0",
    "description": "Taskcaster API",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
        "lint": "eslint 'src/**/*.ts'",
        "lint:write": "npm run lint -- --fix",
        "prettier": "prettier src --check",
        "prettier:write": "npm run prettier -- --write",
        "prlint": "npm run lint && npm run prettier",
        "format": "npm run prettier:write && npm run lint:write"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.27.0",
        "@types/express": "^5.0.2",
        "@types/node": "^22.15.20",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "dotenv": "^16.5.0",
        "eslint": "^9.27.0",
        "eslint-config-prettier": "^10.1.5",
        "express": "^5.1.0",
        "globals": "^16.1.0",
        "install": "^0.13.0",
        "nodemon": "^3.1.10",
        "npm": "^11.4.0",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
